<Page x:Class="InstructorShell.ToolTipPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:InstructorShell"
      mc:Ignorable="d" 
      d:DesignHeight="128" d:DesignWidth="256"
      Title="ToolTipPage">

    <Grid Background="#F9FFFFFF">
        <TextBox Name="textBox" TextAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" TextWrapping="Wrap" BorderBrush="{x:Null}">template</TextBox>
    </Grid>
</Page>
